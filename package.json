{
  "name": "typescript-babel-react-boilerplate",
  "version": "0.0.0",
  "description": "Personal Typescript Babel React Boilerplate",
  "author": "Nicolas Jakob <nicow.jakob@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/njakob/typescript-babel-react-boilerplate",
  "repository": "https://github.com/njakob/typescript-babel-react-boilerplate",
  "bugs": {
    "url": "https://github.com/njakob/typescript-babel-react-boilerplate/issues"
  },
  "keywords": [
    "boilerplate",
    "react",
    "typescript",
    "babel",
    "eslint"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "type-check": "tsc --noEmit -p ./src/@ && tsc --noEmit -p ./src/@scripts",
    "test": "npm run lint && npm run type-check",
    "start": "node -r ./register ./node_modules/.bin/webpack-dev-server --config src/template/scripts/webpack.dev.js",
    "build": "node -r ./register ./node_modules/.bin/webpack --config src/template/scripts/webpack.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.11",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
