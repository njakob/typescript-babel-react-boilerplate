{
  "name": "typescript-babel-react-boilerplate",
  "version": "0.0.0",
  "description": "Personal Typescript Babel React Boilerplate",
  "author": "Nicolas Jakob <nicow.jakob@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/njakob/typescript-babel-react-boilerplate",
  "repository": "https://github.com/njakob/typescript-babel-react-boilerplate",
  "bugs": {
    "url": "https://github.com/njakob/typescript-babel-react-boilerplate/issues"
  },
  "keywords": [
    "boilerplate",
    "react",
    "typescript",
    "babel",
    "eslint"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "type-check": "tsc --noEmit -p ./src/@ && tsc --noEmit -p ./src/@scripts",
    "test": "npm run lint && npm run type-check",
    "start": "node -r ./register ./node_modules/.bin/webpack-dev-server --config src/@scripts/webpack.dev.js",
    "build": "node -r ./register ./node_modules/.bin/webpack --config src/@scripts/webpack.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.2",
    "@types/webpack-env": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "styled-components": "^5.0.0",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}